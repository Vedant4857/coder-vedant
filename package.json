{
  "name": "codecritic",
  "displayName": "CodeCritic",
  "description": "AI powered multi-language code reviewer and fixer for VS Code. Supports C, C++, Python, JavaScript, TypeScript, HTML, CSS, Java.",
  "version": "0.0.1",
  "publisher": "vedant-Shekhar",
  "icon": "icon1.png",
  "repository": {
  "type": "github",
  "url": "https://github.com/Vedant4857/coder-vedant"
}
,
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:codecritic.runReview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "codecritic.runReview",
        "title": "CodeCritic: Run AI Code Review"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "dependencies": {
    "@google/genai": "^1.34.0"
  }
}
